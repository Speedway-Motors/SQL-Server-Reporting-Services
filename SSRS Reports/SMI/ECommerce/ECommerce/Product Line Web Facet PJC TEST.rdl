  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>tngLive</DataSourceName>
        <CommandText>SELECT B.ixSOPBrand
     , B.sBrandName
     , PL.ixSOPProductLine 
     , PL.sTitle
     , SV.ixSOPSKU
     , SB.sName 
FROM tblskubase SB 
LEFT JOIN tblskuvariant SV ON SV.ixSKUBase = SB.ixSKUBase 
LEFT JOIN tblproductline PL ON PL.ixProductLine = SB.ixProductLine 
LEFT JOIN tblbrand B ON B.ixBrand = PL.ixBrand 
WHERE B.ixSOPBrand = '10015' 
  AND PL.ixSOPProductLine = '2316' -- IN (@ixSOPProductLine) -- create new dataset that populates available product line options after narrowing by brand = '1533'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ixSOPBrand">
          <DataField>ixSOPBrand</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sBrandName">
