  <DataSets>
    <DataSet Name="SMI">
      <Query>
        <DataSourceName>SMI</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
	  count(distinct(O.ixCustomer)) as 'TotalCusts', 
	  count(distinct(NewCust.ixOrder)) as 'NewCusts',
      sum(OL.mExtendedPrice) as 'Sales',
      sum(case when OL.mExtendedPrice &lt;&gt; '0' THEN OL.mExtendedCost END) as 'COGS',
      sum(case when OL.mExtendedPrice &lt;&gt; '0' THEN OL.iQuantity END) as 'QTY'
from
      tblOrder O
      left join tblOrderLine OL on O.ixOrder = OL.ixOrder
      left join vwNewCustOrder NewCust on O.ixOrder = NewCust.ixOrder         
where
	 (O.dtOrderDate &gt;= @StartDate and O.dtOrderDate &lt;= @EndDate)
	  and
      O.sOrderChannel in ('AUCTION','EBAY')
      and
      (((OL.flgLineStatus = 'Shipped' or OL.flgLineStatus = 'Open' or OL.flgLineStatus = 'Dropshipped') and (O.sOrderStatus = 'Open' or O.sOrderStatus = 'Shipped'))
            or
      ((OL.flgLineStatus = 'Open' or OL.flgLineStatus = 'Backordered') and O.sOrderStatus = 'Backordered'))</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="TotalCusts">
          <DataField>TotalCusts</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="NewCusts">
          <DataField>NewCusts</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Sales">
          <DataField>Sales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="COGS">
          <DataField>COGS</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="QTY">
          <DataField>QTY</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="OrderDate">
      <Query>
        <DataSourceName>SMI</DataSourceName>
        <CommandText>select distinct(dtOrderDate)
from tblOrder
where 
dtOrderDate &gt;= '06/01/11'
order by dtOrderDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="dtOrderDate">
          <DataField>dtOrderDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
