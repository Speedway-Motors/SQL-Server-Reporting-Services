  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SMISQLLIVE</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CustomerType">
            <Value>=Parameters!CustomerType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SKU">
            <Value>=Parameters!SKU.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
select
	O.ixCustomer as 'Customer',
	O.ixOrder + ' (' + (convert(varchar, O.dtOrderDate, 101)) + ')' as 'Order#',
	--convert(varchar, O.dtOrderDate, 101) as 'Order Date',
	sum(OL.iQuantity) as 'Quantity',
	sum(OL.mExtendedPrice) as 'Extended Price',
	sum(OL.mExtendedCost) as 'Extended Cost'
from
	tblOrderLine OL
	left join tblOrder O on OL.ixOrder = O.ixOrder
	left join tblCustomer C on O.ixCustomer = C.ixCustomer
where
	C.sCustomerType = @CustomerType
	and (OL.dtOrderDate &gt;= @StartDate and OL.dtOrderDate &lt;= @EndDate)
	and (OL.flgLineStatus = 'Shipped' or OL.flgLineStatus = 'Open')
	and OL.ixSKU = @SKU
	and OL.flgKitComponent = 0
group by
	O.ixCustomer, O.ixOrder, convert(varchar, O.dtOrderDate, 101)

	
	
UNION 

/*  per Jason Danley and the Smith's request Eagle Motorsports/Mike Long should now be shown within PRS 
results on these reports and is done using the following query addition */

select
	O.ixCustomer as 'Customer',
	O.ixOrder + ' (' + (convert(varchar, O.dtOrderDate, 101)) + ')' as 'Order#',
	--convert(varchar, O.dtOrderDate, 101) as 'Order Date',
	sum(OL.iQuantity) as 'Quantity',
	sum(OL.mExtendedPrice) as 'Extended Price',
	sum(OL.mExtendedCost) as 'Extended Cost'
from
	tblOrderLine OL
	left join tblOrder O on OL.ixOrder = O.ixOrder
	left join tblCustomer C on O.ixCustomer = C.ixCustomer
where
	--@CustomerType = 'PRS' 
	'PRS' IN (@CustomerType)
	and O.ixCustomer IN ('1073350', '1155856') 
	and (OL.dtOrderDate &gt;= @StartDate and OL.dtOrderDate &lt;= @EndDate)
	and (OL.flgLineStatus = 'Shipped' or OL.flgLineStatus = 'Open')
	and OL.ixSKU = @SKU
	and OL.flgKitComponent = 0	
group by
	O.ixCustomer, O.ixOrder, convert(varchar, O.dtOrderDate, 101)
order by 
	O.ixCustomer, O.ixOrder + ' (' + (convert(varchar, O.dtOrderDate, 101)) + ')'	
	</CommandText>
      </Query>
