      <Query>
        <DataSourceName>SMISQLLIVE</DataSourceName>
        <CommandText>/* Active BOMs with Discontinued Components.rdl
CHANGELOG
Case                 Version By   Action
SMIHD-24195 22.8.1    PJC Report created
*/

SELECT M.ixFinishedSKU 'FinishedSKU', 
	S1.dtDiscontinuedDate 'FinishedSKUDiscontinuedDate',
	D.ixSKU 'ComponentSKU',
	S2.dtDiscontinuedDate 'ComponentSKUDiscontinuedDate',
	D.iQuantity 'Quantity'
FROM tblBOMTemplateMaster M
	left join tblBOMTemplateDetail D on M.ixFinishedSKU = D.ixFinishedSKU
	left join tblSKU S1 on M.ixFinishedSKU = S1.ixSKU
	left join tblSKU S2 on D.ixSKU = S2.ixSKU
WHERE M.flgDeletedFromSOP = 0
	and D.flgDeletedFromSOP = 0 -- 415,012
	and S1.flgActive = 1	    -- 310,793
	and S2.flgActive = 0 -- 13,280
ORDER BY M.ixFinishedSKU, D.ixSKU</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="FinishedSKU">
          <DataField>FinishedSKU</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FinishedSKUDiscontinuedDate">
          <DataField>FinishedSKUDiscontinuedDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="ComponentSKU">
