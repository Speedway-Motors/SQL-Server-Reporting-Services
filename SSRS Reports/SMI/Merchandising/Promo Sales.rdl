  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>dw_speedway2_com</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PromoID">
            <Value>=Parameters!PromoID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- SMIHD-11170 - New Promo report 11/27/18
--  ver 20.53.1
/*    WHAT DO THESE FLAGS MEAN?
      flgPhone	flgWeb	flgStore	flgGlobal	 flgEnabled

DECLARE @PromoID varchar(15),   @StartDate datetime,        @EndDate datetime
SELECT  @PromoID = '1818' ,     @StartDate = '12/14/20',    @EndDate = '12/16/20'   -- 15 sec       
*/

SELECT -- PROMO LEVEL
    TP.ixPromotion 'PromoID', -- aka TopLevelPromoID
    --TP.dtCreateUtc, TP.dtLastUpdateUtc, TP.sCreateUser, TP.sUpdateUser, TP.sPublicName, 
    TP.sPromoCode, 
    CONVERT(VARCHAR,TP.dtStartDateTimeUtc, 1) 'StarDate', --'PromoOfferStart', 
    CONVERT(VARCHAR,TP.dtEndDateTimeUtc, 1) 'EndDate', -- 'PromoOfferEnd'
    --TP.sPrequalifiedMessage, TP.sAppliedMessage, TP.sMarketingMessage, TP.flgPhone, TP.flgWeb, TP.flgStore, TP.flgGlobal, TP.flgEnabled,
            -- PROMO OFFER LEVEL
    TP.ixPromotionOffer 'ReportingPromoID', -- switch to this as the source instead of PCM.ixPromoID after testing
    TP.sPromoName as 'PromoName', -- SIMILAR to PCM.sDescription
    O.PrePromoMerch,
    ISNULL(TPM.TotMerchDiscount,0)      as 'MerchDiscount',
    ISNULL(O.MerchTotal,0)              as 'Sales', 
    ISNULL(O.CountOrders,0)             as 'CountOrders',
    ISNULL(TPS.TotShippingDiscount,0)   as 'TotSHDiscount',   
    ISNULL(TPS.TotPrePromoSHCharge,0)   as 'TotPrePromoSHCharge',
    ISNULL(TPS.TotPostPromoSHCharge,0)  as 'TotPostPromoSHCharge', 
    -- CM$ = (Sales	- COGS  - Est Fulfillment Cost - (mPrePromoShippingCharge * .7) + mPostPromoShippingCharge
    -- CM% = =Iif((MerchTotal)=0,"N/A",((MerchTotal-MerchCost-(PrePromoMerch*.1)-(TotPrePromoSHCharge*.7)+TotPostPromoSHCharge)/(MerchTotal+TotPostPromoSHCharge)))
    ISNULL(O.MerchCost,0)               as 'MerchCost',
    ISNULL(O.CountCustomers,0)          as 'CountCustomers',
    O.CountNewCustomers,
    CRITOFF.Criteria AS 'Offfer Criteria data',
    CRITOFF.Offer AS 'Offer Incentive data'
FROM (-- Distinct List of Promotion IDs and PromotionOffer IDs
--DECLARE @PromoID varchar(15),   @StartDate datetime,        @EndDate datetime
--SELECT  @PromoID = '1818' ,     @StartDate = '12/14/20',    @EndDate = '12/16/20'   -- 15 sec 
      SELECT distinct TP.ixPromotion, PO.ixPromotionOffer, dtStartDateTimeUtc, dtEndDateTimeUtc,
        TP.sPromoCode, TP.sPromoName
        --ixPromoCode,  &lt;-- this was causing a cartesian on a small # of PromoIDs e.g. PromoId 1410
      FROM Promo.tblpromotion TP 
         left join Promo.tblpromotion_offer PO on TP.ixPromotion = PO.ixPromotion
      WHERE TP.ixPromotion in (@PromoID)
      )TP
      LEFT JOIN(-- calc the TOTAL PROMO MERCH discount
--DECLARE @PromoID varchar(15),   @StartDate datetime,        @EndDate datetime
--SELECT  @PromoID = '1818' ,     @StartDate = '12/14/20',    @EndDate = '12/16/20'   -- 15 sec    
                SELECT SP.ixPromoId as 'ixPromoId',
                    sum(case when SP.flgAddedToCart=1 then SP.mExtendedCost else SP.mExtendedPrePromoPrice end)-sum(SP.mExtendedPostPromoPrice) as 'TotMerchDiscount'
                 FROM tblSKUPromo SP 
                    left join tblOrder O on O.ixOrder = SP.ixOrder  
                 WHERE O.dtOrderDate between @StartDate and @EndDate               
                     and O.sOrderStatus = 'Shipped'
                     and O.mMerchandise &gt; 0
                     and len(SP.ixPromoId) &lt; 5 -- hack to avoid contactinated promos (2 or 3 applies to same order!)
                 GROUP BY SP.ixPromoId
                 ) TPM on TPM.ixPromoId = TP.ixPromotionOffer
    /* NEED THE JOIN ISSUE RESOLVED!!! 
       tblShippingPromo.ixPromoId VARCHAR(15)  to  Promo.tblpromotion_offer.ixPromotionOffer (INT)    
       'The conversion of the varchar value '''76376316531653''' overflowed an int column */
      left join ( --SubQuery to calc the TOTAL PROMO SHIPPING discount
--DECLARE @PromoID varchar(15),   @StartDate datetime,        @EndDate datetime
--SELECT  @PromoID = '1818' ,     @StartDate = '12/14/20',    @EndDate = '12/16/20'   -- 15 sec 
                SELECT SP.ixPromoId as ixPromoId,
                    sum(isnull(SP.mPrePromoShippingCharge,0)-(isnull(SP.mPostPromoShippingCharge,0)) )  as 'TotShippingDiscount',
                    sum(isnull(SP.mPrePromoShippingCharge,0)) as 'TotPrePromoSHCharge',
                    sum(isnull(SP.mPostPromoShippingCharge,0)) as 'TotPostPromoSHCharge'
                 FROM tblShippingPromo SP 
                    left join tblOrder O on O.ixOrder = SP.ixOrder 
                 WHERE O.dtOrderDate between @StartDate and @EndDate                     
                     and O.sOrderStatus = 'Shipped'
                     and O.mMerchandise &gt; 0 -- merch &gt; 0 is needed to avoid pulling in zero-shipped CRAP
                 GROUP BY SP.ixPromoId
                 ) TPS on TPS.ixPromoId = TP.ixPromotionOffer
    LEFT JOIN( -- ORDER SUMMARY BY Promo Offer
--DECLARE @PromoID varchar(15),   @StartDate datetime,        @EndDate datetime
--SELECT  @PromoID = '1818' ,     @StartDate = '12/14/20',    @EndDate = '12/16/20'   -- 15 sec 
                SELECT PO.ixPromotionOffer,
                    COUNT(distinct(SUBSTRING(O.ixMasterOrderNumber, 1, 7)))   as 'CountOrders',
                    COUNT(distinct(O.ixCustomer))                 as 'CountCustomers',
                    SUM(isnull(O.mMerchandise,0))                 as 'MerchTotal', 
                    SUM(isnull(O.mMerchandiseCost,0))             as 'MerchCost',
                    SUM(isnull(CPS.PrePromoMerchandise,0))        as 'PrePromoMerch',
                    SUM(case when FO.ixCustomer is NULL then 0
                       else 1 end)                                as 'CountNewCustomers'
                FROM Promo.tblpromotion_offer PO
                    LEFT JOIN tblOrderPromoCodeXref OPCXREF on OPCXREF.ixPromoID = PO.ixPromotionOffer
                    LEFT JOIN tblOrder O on O.ixOrder = OPCXREF.ixOrder
                    LEFT JOIN vwOrderCombinedPromoSummary CPS on O.ixOrder = CPS.ixOrder AND O.mMerchandise &gt; 0 -- merch &gt; 0 is needed to avoid pulling in zero-shipped CRAP
--                      pulls FROM vwOrderLinePromoSummary &amp; vwOrderShippingPromoSummary
--                                 (tblSKUPromo &amp; tblOrder)  (tblShippingPromo &amp; tblOrder)
                    LEFT JOIN  (-- First Order
                                SELECT ixCustomer, MIN(ixOrderDate) ixFirstOrderDate, MIN(dtOrderDate) dtFirstOrderDate, COUNT(O.ixOrder) OrderCnt
                                FROM tblOrder O
                                WHERE O.sOrderStatus = 'Shipped'
                                    and O.mMerchandise &gt; 0 -- &gt; 1 if looking at non-US orders
                                    and O.sOrderType &lt;&gt; 'Internal'   -- USUALLY filtered
                                GROUP BY ixCustomer) FO on FO.ixCustomer = O.ixCustomer AND O.ixOrderDate = FO.ixFirstOrderDate -- AND O.mMerchandise &gt; 0-- merch &gt; 0 is needed to avoid pulling in zero-shipped CRAP
                WHERE O.dtOrderDate between @StartDate and @EndDate
                                          and O.sOrderStatus = 'Shipped'
                                          and O.sOrderType &lt;&gt; 'Internal'  
                                          and O.mMerchandise &gt; 0  
                GROUP BY PO.ixPromotionOffer
                ) O ON O.ixPromotionOffer = TP.ixPromotionOffer
    LEFT JOIN (-- CRITERIA and OFFER FIELDS
select
	p.ixPromotion, p.sPublicName
	, po.ixPromotionOffer
	, Criteria.Criteria
	, Offer.Offer
from Promo.tblpromotion p
    inner join Promo.tblpromotion_offer po on p.ixPromotion = po.ixPromotion and LEN(p.ixPromotion) &lt; 5
    -- The parts below get the criteria and offer
    left join    (-- Criteria
	                select 
		                a.ixPromotionOffer
		                , string_agg(
		                case when a.sPromotionOfferCriteriaTypeName in ('Merchandise Total') then a.sPromotionOfferCriteriaTypeName + ' ' + a.[sPromotionOfferCriteriaOperator] + ' ' +  '$' + a.sPromotionOfferCriteriaEnteredValue
		                when a.sPromotionOfferCriteriaTypeName in ('Product In Sku List','Product In Part Type List', 'Product In Brand List') then a.sPromotionOfferCriteriaTypeName 
		                else '' end, ', ')
		                as Criteria
	                from (select 
			                distinct 
			                po.ixPromotionOffer
			                , POCT.sPromotionOfferCriteriaTypeName
			                , poco.[sPromotionOfferCriteriaOperator]
			                , case when POCT.sPromotionOfferCriteriaTypeName in ('Merchandise Total') then poce.sPromotionOfferCriteriaEnteredValue
			                when POCT.sPromotionOfferCriteriaTypeName in ('Product In Sku List','Product In Part Type List', 'Product In Brand List') then POCT.sPromotionOfferCriteriaTypeName
			                else 'Unknown'
			                end as sPromotionOfferCriteriaEnteredValue
		                from Promo.tblpromotion p
		                inner join Promo.tblpromotion_offer po on p.ixPromotion = po.ixPromotion
		                inner join Promo.tblpromotion_offer_criteria poc on poc.ixPromotionOffer = po.ixPromotionOffer
		                inner join Promo.[tblpromotion_offer_criteria_type] poct on poct.ixPromotionOfferCriteriaType = poc.ixPromotionOfferCriteriaType
		                left join Promo.[tblpromotion_offer_criteria_enteredvalue] poce on poce.ixPromotionOfferCriteria = poc.ixPromotionOfferCriteria
		                left join Promo.[tblpromotion_offer_criteria_operator] poco on poco.ixPromotionOfferCriteriaOperator = poc.ixPromotionOfferCriteriaOperator
                --		order by ixPromotionOffer
	                    ) a
	                group by a.ixPromotionOffer
                ) Criteria on Criteria.ixPromotionOffer = po.ixPromotionOffer
                left join (-- Offer
	                    select po.ixPromotionOffer
		                    , string_agg(
		                    case 
			                    when sPromotionOfferIncentiveTypeName = '$ off Merchandise Total' then replace(POCT.sPromotionOfferIncentiveTypeName,'$','$' + poce.sPromotionOfferIncentiveValue) 
			                    when sPromotionOfferIncentiveTypeName = '% off Shipping' then replace(POCT.sPromotionOfferIncentiveTypeName,'%',poce.sPromotionOfferIncentiveValue + '%') 
			                    else poce.sPromotionOfferIncentiveValue  + ' ' + POCT.sPromotionOfferIncentiveTypeName end,',')  as Offer
	                    from Promo.tblpromotion p
	                    inner join Promo.tblpromotion_offer po on p.ixPromotion = po.ixPromotion
	                    inner join Promo.tblpromotion_offer_incentive poc	on poc.ixPromotionOffer = po.ixPromotionOffer
	                    inner join Promo.[tblpromotion_offer_incentive_type] poct	on poct.ixPromotionOfferIncentiveType = poc.ixPromotionOfferIncentiveType
	                    left join Promo.[tblpromotion_offer_incentive_value] poce on poce.ixPromotionOfferIncentive = poc.ixPromotionOfferIncentive
	                    group by po.ixPromotionOffer
                      ) Offer on Offer.ixPromotionOffer = po.ixPromotionOffer
            ) CRITOFF on CRITOFF.ixPromotionOffer = TP.ixPromotionOffer
ORDER BY TP.ixPromotion desc, TP.ixPromotionOffer</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
