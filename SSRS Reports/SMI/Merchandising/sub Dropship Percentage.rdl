  <DataSets>
    <DataSet Name="SMI">
      <Query>
        <DataSourceName>SMIDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*
DECLARE
    @Start_Date datetime,
    @End_Date datetime

SELECT
    @Start_Date = '05/01/14',
    @End_Date = '05/31/14'  
*/    
SELECT COUNT(DISTINCT O.ixOrder) AS OrdCount 
     , SUM(OL.mExtendedPrice) AS GSSales
     , SUM(O.mMerchandise) AS TotalSales
     , SUM(OL.mExtendedPrice)/SUM(O.mMerchandise) AS GSPercentage
FROM tblOrderLine OL
LEFT JOIN vwSKULocalLocation SKU ON SKU.ixSKU = OL.ixSKU
LEFT JOIN tblOrder O ON O.ixOrder = OL.ixOrder
WHERE OL.dtShippedDate BETWEEN @StartDate AND @EndDate 			
  AND OL.flgLineStatus = 'Dropshipped'
  AND O.sOrderType &lt;&gt; 'INTERNAL'
  AND OL.flgKitComponent = 0 -- KIT COMPONENT CHECK</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="OrdCount">
          <DataField>OrdCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="GSSales">
          <DataField>GSSales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalSales">
          <DataField>TotalSales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="GSPercentage">
          <DataField>GSPercentage</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
