  <DataSets>
    <DataSet Name="SMI">
      <Query>
        <DataSourceName>SMI</DataSourceName>
        <CommandText>select SKU.ixSKU, SKU.sDescription SKUDescription, KL.mAvgKitCost
from tblSKU SKU
    join tblKitList KL on KL.ixKitSKU = SKU.ixSKU
--where mAvgKitCost is not null
WHERE SKU.flgDeletedFromSOP = '0'
order by SKU.ixSKU</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ixSKU">
          <DataField>ixSKU</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SKUDescription">
          <DataField>SKUDescription</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="mAvgKitCost">
          <DataField>mAvgKitCost</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
