  <DataSets>
    <DataSet Name="SMI">
      <Query>
        <DataSourceName>SMI</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Date">
            <Value>=Parameters!Date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*
	Vendor Terms &amp; Conditions from SOP/MAP (Freight terms, discounts, nets, etc)
	Exclude Speedway’s overseas and private label vendors
*/

select 
       MV.ixVendor      VendorNum,
       MV.sName         VendorName,
       MV.sSalesRep    SalesRep,
       MV.sPaymentTerms PaymentTerms,
       MV.sFreightTerms   FreightTerms,
       MV.sPreferredMOS PrefMoS,
       TY.Dollars       TYDollars,
       YRminus1.Dollars     YRminus1Dollars,
       YRminus2.Dollars     YRminus2Dollars,
       YRminus3.Dollars     YRminus3Dollars    
           
FROM
     -- ANY vendor that we received from in the past 4(approx) years
     (select distinct V.ixVendor, V.sName,  
        V.sSalesRep,
        V.sPaymentTerms, 
        V.sFreightTerms, 
        V.sPreferredMOS
      from tblPODetail POD
        left join tblPOMaster POM   on POM.ixPO = POD.ixPO
        left join tblVendor V on V.ixVendor = POM.ixVendor
        left join tblDate D on D.ixDate = POD.ixFirstReceivedDate
      where D.dtDate &gt; DATEADD(yy, DATEDIFF(yy,0,@Date)-3, 0)
        and D.dtDate &lt; @Date
        and POM.flgIssued = 1
        ) MV
               -- CURRENT YTD
    FULL JOIN(select  datepart(Year,D.dtDate)           YR,
                    POM.ixVendor                        VendorNum,
                    V.sName                             VendorName,
                    SUM(POD.iQuantityPosted * POD.mCost)    Dollars
            from tblPODetail POD
                left join tblPOMaster POM   on POM.ixPO = POD.ixPO
                left join tblDate D         on D.ixDate = POD.ixFirstReceivedDate
                left join tblVendor V       on V.ixVendor = POM.ixVendor
            WHERE D.dtDate &gt;= DATEADD(yy, DATEDIFF(yy,0,@Date), 0)    -- first of the year for @Date
                  and D.dtDate &lt; @Date -- @Date
            group by datepart(Year,D.dtDate),
                    POM.ixVendor,
                    V.sName
            ) TY on TY.VendorNum = MV.ixVendor  

    -- LAST YR (full year)
    FULL JOIN(select  datepart(Year,D.dtDate)           YR,
                    POM.ixVendor                        VendorNum,
                    SUM(POD.iQuantityPosted * POD.mCost)    Dollars
            from tblPODetail POD
                left join tblPOMaster POM   on POM.ixPO = POD.ixPO
                left join tblDate D         on D.ixDate = POD.ixFirstReceivedDate
                left join tblVendor V       on V.ixVendor = POM.ixVendor
            WHERE D.dtDate &gt;= DATEADD(yy, DATEDIFF(yy,0,@Date)-1, 0)    -- first of the year for @Date -1 year
                  and D.dtDate &lt; DATEADD(yy, DATEDIFF(yy,0,@Date), 0)    -- first of the year for @Date
            group by datepart(Year,D.dtDate),
                    POM.ixVendor
            ) YRminus1 on YRminus1.VendorNum = MV.ixVendor  

    -- 2 YRS ago (full year)
    FULL JOIN(select  datepart(Year,D.dtDate)           YR,
                    POM.ixVendor                        VendorNum,
                    SUM(POD.iQuantityPosted * POD.mCost)    Dollars
            from tblPODetail POD
                left join tblPOMaster POM   on POM.ixPO = POD.ixPO
                left join tblDate D         on D.ixDate = POD.ixFirstReceivedDate
                left join tblVendor V       on V.ixVendor = POM.ixVendor
        WHERE D.dtDate &gt;= DATEADD(yy, DATEDIFF(yy,0,@Date)-2, 0)     -- first of the year for @Date -2 year  
              and D.dtDate &lt;  DATEADD(yy, DATEDIFF(yy,0,@Date)-1, 0)     -- first of the year for @Date -1 year
        group by datepart(Year,D.dtDate),
                POM.ixVendor

        ) YRminus2 on YRminus2.VendorNum = MV.ixVendor  
    --3 YRS ago (full year)
    FULL JOIN(select  datepart(Year,D.dtDate)           YR,
                    POM.ixVendor                        VendorNum,
                    SUM(POD.iQuantityPosted * POD.mCost)    Dollars
            from tblPODetail POD
                left join tblPOMaster POM   on POM.ixPO = POD.ixPO
                left join tblDate D         on D.ixDate = POD.ixFirstReceivedDate
                left join tblVendor V       on V.ixVendor = POM.ixVendor
        WHERE D.dtDate &gt;= DATEADD(yy, DATEDIFF(yy,0,@Date)-3, 0)-- first of the year for @Date -3 years
              and D.dtDate &lt; DATEADD(yy, DATEDIFF(yy,0,@Date)-2, 0) -- first of the year for @Date -2 years
        group by datepart(Year,D.dtDate),
                POM.ixVendor
        ) YRminus3 on YRminus3.VendorNum = MV.ixVendor  
order by TY.Dollars desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
