  <DataSets>
    <DataSet Name="SMI">
      <Query>
        <DataSourceName>SMISQLLIVE</DataSourceName>
        <CommandText>SELECT ixPO, count(*) Lines, count(distinct iOrdinality) DifOrdinalities, max(iOrdinality) MaxOrdinality
FROM tblPODetail
GROUP BY ixPO
HAVING ((count(*) &lt;&gt; count(distinct iOrdinality)) 
    OR (count(*) &lt;&gt; max(iOrdinality))
) 
AND  ixPO &lt;&gt; '23447' -- old PO missing single row in SOP
ORDER BY ixPO</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ixPO">
          <DataField>ixPO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Lines">
          <DataField>Lines</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DifOrdinalities">
          <DataField>DifOrdinalities</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="MaxOrdinality">
          <DataField>MaxOrdinality</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
