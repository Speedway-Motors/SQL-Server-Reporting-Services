  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SMISQLLIVE</DataSourceName>
        <CommandText>/* DC - sub05 Table Size Check.rdl
    ver 18.14.1
*/
--SMI
select A.sTableName,
   B.sRowCount  YDayRowCount,
   A.sRowCount  TDayRowCount,
   B.KB         YDayKB,
   A.KB         TDayKB,
   B.MB         YDayMB,
   A.MB         TDayMB
from tblTableSizeLog A
join tblTableSizeLog B
on A.sTableName = B.sTableName
where A.dtDate = DateAdd(day, DateDiff(day, 0, GETDATE()), 0)  -- today
    and B.dtDate = DateAdd(day, DateDiff(day, 0, GETDATE()), -1)   -- yesterday
   and (A.sRowCount &lt; B.sRowCount -- Row count dropped since yday
           OR
        A.sRowCount = 0) -- empty table
    and A.sTableName NOT IN ('tblAwsCheckDestination','tblAwsQueue','tblAwsQueueStage', -- AWS tables
                'tblAwsQueueStage','tblDoorEventArchive','tblDoorEventArchive010114to041414','tblDoorEventArchive041514to123114','tblLatestFeed','tblOrderFreeShippingEligible','tblPromotionalInventory','tblSKUIndex',    -- EMPTY tables to be deleted
                'tblBinSku','tblVelocity60','tblLoanTicketMaster','tblLoanTicketDetail','tblOrderTNT','tblLoanTicketScans','tblStockOut')
order by A.sTableName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
