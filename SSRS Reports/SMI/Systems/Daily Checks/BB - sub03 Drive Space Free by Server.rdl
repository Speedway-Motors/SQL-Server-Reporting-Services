    <DataSet Name="LNKDWSTAGING1">
      <Query>
        <DataSourceName>SMISQLLIVE</DataSourceName>
        <CommandText>CREATE TABLE #drives (
        drive char,
        [free] int
)
        
INSERT INTO #drives
EXEC master..xp_fixeddrives

SELECT 'LNK-DW1' as 'Server'
,drive+':' as 'Drive'
, ([free] /1024) as 'GB Free Space'
FROM #drives 
--WHERE [free] &lt; 90 * 1024 -- less than 25 GB free

DROP TABLE #drives</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
