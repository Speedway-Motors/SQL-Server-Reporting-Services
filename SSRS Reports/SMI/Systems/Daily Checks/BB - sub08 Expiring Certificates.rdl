  <DataSets>
    <DataSet Name="LNK_SQL_LIVE_1">
      <Query>
        <DataSourceName>SMISQLLIVE</DataSourceName>
        <CommandText>-- certificate expirations

SELECT -- LNK-SQL-LIVE-1
    @@SERVERNAME 'Server',
    certificate_id 'Cert ID', subject, 
    FORMAT(expiry_date,'MM/dd/yyyy') 'Expires',
    FORMAT(start_date,'MM/dd/yyyy') 'Started'
FROM master.sys.certificates c 
WHERE expiry_date &lt; DATEADD(day,45,getdate()) -- expires after X days ago
    and name not like '##%'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Server">
          <DataField>Server</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Cert_ID">
          <DataField>Cert ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="subject">
          <DataField>subject</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Expires">
          <DataField>Expires</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Started">
          <DataField>Started</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
