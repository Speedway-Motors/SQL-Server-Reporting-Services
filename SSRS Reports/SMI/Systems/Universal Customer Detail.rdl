  <DataSets>
    <DataSet Name="SMI">
      <Query>
        <DataSourceName>SMI</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CustNum">
            <Value>=Parameters!CustNum.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
   C.ixCustomer              CustNum,
   ixAccountManager        AccountManager,
   sCustomerType           CustType,
   dtAccountCreateDate     CreateDate,
   ixSourceCode            InitialSourceCode,
   iPriceLevel             PriceLevel,
   sMapTermsLongDescription Terms,
   sCustomerFirstName   FirstName,
   sCustomerLastName    LastName,
   sMailToCity          MailToCity,
   sMailToZip           MailToZip,
   sMailToCountry       MailToCountry,
   sEmailAddress        Email,
   flgMarketingEmailSubscription EmailSubscription,
   O.OrdCount,
   O.TotalMerch,
   RETURNS.CMMCount, 
   RETURNS.TotalReturns,
   LO.dtLastOrder,
   LO.LastSourceCode
from tblCustomer C
   left join (select ixCustomer, count(ixOrder) OrdCount,
                            sum(mMerchandise) TotalMerch
                 from tblOrder 
                 where sOrderStatus not in ('Cancelled','Pick Ticket','Quote','Cancelled Quote')
                 group by ixCustomer
                ) O on O.ixCustomer = C.ixCustomer
   left join (select ixCustomer, count(ixCreditMemo) CMMCount,
                           sum(mMerchandise) TotalReturns
                 from tblCreditMemoMaster
                 where flgCanceled = 0
                 group by ixCustomer
                ) RETURNS on RETURNS.ixCustomer = C.ixCustomer
    left join(select top 1 ixCustomer,dtOrderDate dtLastOrder, sSourceCodeGiven LastSourceCode
                 from tblOrder
                 where sOrderStatus not in ('Cancelled','Pick Ticket','Quote','Cancelled Quote')
                    and ixCustomer = @CustNum
                 order by dtOrderDate desc
                ) LO on LO.ixCustomer = C.ixCustomer
where C.ixCustomer = @CustNum</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
