  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SMISQLLIVE</DataSourceName>
        <CommandText>
-- long numeric SKUs that export as scientific notation 
select ixSKU, dtCreateDate from tblSKU
where flgDeletedFromSOP = 0
    and (ISNUMERIC(ixSKU) = 1
            and len(ixSKU) &gt;= 17 -- length of 12 or more export as scientific notation
            and ixSKU NOT LIKE '%.%'
            and ixSKU between '33001752506001300' and '37631177290005043' -- 7 long example SKUs
    )
    OR ixSKU in ('81706','HB8FHCSC-10-24-3.63','UP90163')
order by ixSKU desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ixSKU">
          <DataField>ixSKU</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="dtCreateDate">
          <DataField>dtCreateDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
