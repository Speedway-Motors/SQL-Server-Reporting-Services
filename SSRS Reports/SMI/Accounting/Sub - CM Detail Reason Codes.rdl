  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SMISQLLIVE</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CreditMemo">
            <Value>=Parameters!CreditMemo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct ixReasonCode, sDescription 
from tblCreditMemoReasonCode
where ixReasonCode in (select sReasonCode 
                           from tblCreditMemoDetail 
                           where ixCreditMemo = @CreditMemo)
order by ixReasonCode</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
