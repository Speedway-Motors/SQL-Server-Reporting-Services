  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SMIDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Location">
            <Value>=Parameters!Location.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- NEW VERSION OF Gross Revenue Order and Package Count by Channel.rdl
/*
DECLARE @StartDate datetime,@EndDate datetime,@Location INT
SELECT @StartDate = '01/01/16',@EndDate = '11/02/16',@Location = 99
*/
SELECT  coalesce(CHAN.OrdChan,AllChannelLocation.ixOrderChannel) as OrdChan,
              coalesce(LOC.ixLocation,AllChannelLocation.ixLocation) as ixLocation,
              coalesce(SALES.DailyNumOrds,0) as DailyNumOrds,
              coalesce(SALES.DailySales,0) as DailySales,
          --    coalesce(SALES.OrdChan, AllChannelLocation.ixOrderChannel) as OrdChan,
              coalesce(SALES.PkgCount,0) as PkgCount
FROM  (-- ORDER CHANNEL LIST
       SELECT DISTINCT OrdChan 
       from vwDailyGrossRevAndPkgsByChannel /* 10 total channels   AMAZON    AUCTION    COUNTER    COUNTER P/U    E-MAIL    FAX    INTERNAL    MAIL    PHONE    WEB    */
       ) CHAN 
FULL OUTER JOIN (-- SUMMARY BY CHANNEL &amp; LOCATION
              SELECT OrdChan, ixPrimaryShipLocation,
              SUM(DailySales) DailySales,
              SUM(DailyNumOrds) DailyNumOrds,
              SUM(PkgCount) PkgCount
              FROM vwDailyGrossRevAndPkgsByChannel
              WHERE dtShippedDate between @StartDate and @EndDate
              GROUP BY OrdChan, ixPrimaryShipLocation
              ) SALES on SALES.OrdChan = CHAN.OrdChan 
FULL OUTER JOIN (-- LOCATIONS
              SELECT ixLocation
              from tblLocation
              where ixLocation in (@Location) -- (47,97,98,99)
              ) LOC on LOC.ixLocation = SALES.ixPrimaryShipLocation
right outer join 
(
       select ixOrderChannel, ixLocation from tblOrderChannel cross join tblLocation where ixOrderChannel not in ('COUNTER', 'INTERNAL')
              union
       select 'COUNTER P/U' as ixOrderChannel, ixLocation from tblLocation
) AllChannelLocation on CHAN.OrdChan = AllChannelLocation.ixOrderChannel and LOC.ixLocation = AllChannelLocation.ixLocation
-- WHERE CHAN.OrdChan is NOT NULL
ORDER BY coalesce(CHAN.OrdChan,AllChannelLocation.ixOrderChannel),
coalesce(LOC.ixLocation,AllChannelLocation.ixLocation)</CommandText>
