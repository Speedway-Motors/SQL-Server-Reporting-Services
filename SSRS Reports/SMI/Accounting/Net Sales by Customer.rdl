          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ReturnsMerch">
          <DataField>ReturnsMerch</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ReturnsFreight">
          <DataField>ReturnsFreight</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ReturnsTax">
          <DataField>ReturnsTax</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="NetMerch">
          <DataField>NetMerch</DataField>
