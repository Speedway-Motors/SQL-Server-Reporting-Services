  <DataSets>
    <DataSet Name="SMI">
      <Query>
        <DataSourceName>SMI</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Customer">
            <Value>=Parameters!Customer.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT O.ixCustomer, 
SUM(O.mMerchandise)            GrossMerch, 
SUM(O.mShipping)               GrossFreight,    
SUM(O.mTax)                    GrossTax,
isnull(R.ReturnsMerch,0)   ReturnsMerch,
isnull(R.ReturnsFreight,0)  ReturnsFreight,
isnull(R.ReturnsTax,0)       ReturnsTax,
(SUM(O.mMerchandise)-isNull(R.ReturnsMerch,0)) NetMerch,
(SUM(O.mShipping)-isNull(R.ReturnsFreight,0)) NetFreight,
(SUM(O.mTax)-isNull(R.ReturnsTax,0)) NetTax
FROM tblOrder O
    -- RETURNS
    left join(select ixCustomer,
                    SUM(mMerchandise) ReturnsMerch, 
                    SUM(mShipping)    ReturnsFreight,   
                    SUM(mTax)         ReturnsTax
                from tblCreditMemoMaster CMM
                where dtCreateDate &gt;= @StartDate
                    and dtCreateDate &lt; (@EndDate+1)
                    and flgCanceled = 0
                    and ixCustomer = @Customer
                group by ixCustomer
              ) R on R.ixCustomer = O.ixCustomer
WHERE O.sOrderStatus = 'Shipped'
    and O.dtShippedDate &gt;= @StartDate
    and O.dtShippedDate &lt; (@EndDate+1)
    and O.ixCustomer = @Customer
GROUP BY O.ixCustomer ,
R.ReturnsMerch,
R.ReturnsFreight,
R.ReturnsTax</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ixCustomer">
          <DataField>ixCustomer</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="GrossMerch">
          <DataField>GrossMerch</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="GrossFreight">
          <DataField>GrossFreight</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="GrossTax">
