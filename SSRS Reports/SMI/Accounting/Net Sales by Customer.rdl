    <DataSet Name="Customer">
      <Query>
        <DataSourceName>SMI</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Customer">
            <Value>=Parameters!Customer.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select (isNull(sCustomerFirstName,' ')+' '+isNull(sCustomerLastName,' ')) CustomerName
from tblCustomer
where ixCustomer = @Customer</CommandText>
