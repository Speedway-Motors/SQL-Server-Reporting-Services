    <DataSet Name="PromoIdList">
      <Query>
        <DataSourceName>SMIDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct ixPromoId
from tblPromoCodeMaster     
where dtStartDate &lt;= @EndDate -- promo starts before date range ends
   AND dtEndDate &gt;= @StartDate  -- promo ends after date range starts
   AND ixPromoId NOT like 'Z%'
     </CommandText>
      </Query>
