  <DataSets>
    <DataSet Name="SMI">
      <Query>
        <DataSourceName>SMISQLLIVE</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct (Case when sJob like '81-8%' then '81-8'
          when sJob like '81-9%' then '81-9' 
          else sJob
     end) sJob, JobDescription, sJobSort,
    (sum(iTotDailyJobTime) /3600.00) JobTime
from vwDailyTotJobTime
where  sJob like '81%'
--(ixDepartment = '81' OR ixEmployee = 'AAI') -- Tony Itzen
    and dtDate &gt;= @StartDate
    and dtDate &lt; (@EndDate+1)
group by sJob, JobDescription, sJobSort
order by  sJobSort</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="sJob">
          <DataField>sJob</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="JobDescription">
          <DataField>JobDescription</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sJobSort">
          <DataField>sJobSort</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="JobTime">
          <DataField>JobTime</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
