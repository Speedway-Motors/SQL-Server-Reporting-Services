  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SMISQLLIVE</DataSourceName>
        <CommandText>-- Canceled Counter Orders
SELECT DISTINCT O.ixOrder 'Order', O.dtOrderDate 'OrderDate'
    , (ISNULL(C.sCustomerFirstName,'') + ' ' + ISNULL(C.sCustomerLastName,'')) 'Customer'
    , O.sOrderTaker 'OrderTaker'
    --, O.sOrderType 'Type'    , O.sSourceCodeGiven 'SourceCode',  O.sOrderStatus 'Status'
FROM tblOrder O
    left join tblCounterOrderScans CS on O.ixOrder = CS.ixOrder
    left join tblCustomer C on O.ixCustomer = C.ixCustomer
WHERE ixOrderDate &gt;=19750 -- reset periodically
    and  iShipMethod = 1   
    and dtOrderDate &gt;= DATEADD(dd, -10, getdate())  -- X Days ago
    and sOrderStatus = 'Cancelled'
    and CS.ixOrder is NOT NULL --(only care if someone started pulling the items)
ORDER BY dtOrderDate desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Order">
          <DataField>Order</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OrderDate">
          <DataField>OrderDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Customer">
          <DataField>Customer</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OrderTaker">
          <DataField>OrderTaker</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
