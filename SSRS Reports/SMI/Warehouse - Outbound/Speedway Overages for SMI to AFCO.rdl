  <DataSets>
    <DataSet Name="SMI">
      <Query>
        <DataSourceName>SMIDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>USE [AFCOReporting]

/******************************************************************************
 ************** be careful when looking back and forth between SMI &amp; AFCO,                    ****
 **************  make sure you're grabbing the right field values                                          ****
 ******************************************************************************/
select distinct
    PO.ixPO              AFCOPO,
VS.sVendorSKU       SMISKU,
    PO.ixSKU            AFCOSKU,
isnull(SKU.sDescription,'* '+AFCOSKU.sDescription)     SKUDescription,
    PO.QTYOrd,
    PO.QTYRec,
    PO.Excess,
(PO.Excess * isnull(SKU.mAverageCost,AFCOSKU.mAverageCost)) CostLostInv,
VS.iOrdinality
from
    (select 
           POM.ixVendor,
           POM.ixPO,
           POD.ixSKU, 
           isnull(POD.iQuantity,0)                          QTYOrd, 
           POD.iQuantityPosted                              QTYRec,
           (POD.iQuantityPosted-isnull(POD.iQuantity,0))    Excess
        from tblPODetail POD
             join tblPOMaster POM on POM.ixPO = POD.ixPO
             join tblDate D on D.ixDate = POD.ixFirstReceivedDate
                       and D.dtDate &gt;= @StartDate
                       and D.dtDate &lt; (@EndDate+1)
        where POD.iQuantityPosted &gt; isnull(POD.iQuantity,0)
          and POM.ixVendor = '5302'-- SPEEDWAY MOTORS, INC
        ) PO -- on PO.ixPO = R.ixPO
    join tblVendorSKU VS on VS.ixSKU COLLATE SQL_Latin1_General_CP1_CS_AS = PO.ixSKU COLLATE SQL_Latin1_General_CP1_CS_AS
    left join [SMI Reporting].dbo.vwSKULocalLocation SKU on SKU.ixSKU COLLATE SQL_Latin1_General_CP1_CS_AS = VS.sVendorSKU COLLATE SQL_Latin1_General_CP1_CS_AS
    left join vwSKULocalLocation AFCOSKU on AFCOSKU.ixSKU COLLATE SQL_Latin1_General_CP1_CS_AS = VS.ixSKU COLLATE SQL_Latin1_General_CP1_CS_AS
where VS.ixVendor = '5302'
order by  PO.ixPO,PO.ixSKU</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
