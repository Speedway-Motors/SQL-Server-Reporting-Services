    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="SMI">
      <Query>
        <DataSourceName>SMISQLLIVE</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Employee">
            <Value>=Parameters!Employee.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/***	Summary of Changes to Daylight Saving Time ***
	    Prior to 2007	        2007 and After	
Start	1st SUN in April	    2nd SUN in March	
End	    Last SUN in October	    1st SUN in November	
				
				
------------ Start ------ End ----- Year
Door System	04/06/14	10/26/14	2014
Actual US	03/09/14	11/02/14	2014
			
Door System	04/05/15	10/25/15	2015
Actual US	03/08/15	11/01/15	2015
			
Door System	04/03/16	10/30/16	2016
Actual US	03/13/16	11/06/16	2016
			
Door System	04/02/17	10/29/17	2017
Actual US	03/12/17	11/05/16	2017
*/	
SELECT distinct
    E.sFirstname,
    E.sLastname,
    E.ixEmployee, -- placeholder until Employee# is created
    E.sPayrollId,
    E.ixDepartment,
    CONVERT(VARCHAR(10), DE.dtEventTimeDate, 101) AS [MM/DD/YYYY],
    DE.sAction,
-- MODIFY THE CASE STATEMENT BASED ON THE CHART AT THE TOP OF THE COMMENTS TO COMPENSATE FOR OUR JUNKY DOOR SYSTEM
    (Case when (DE.dtEventTimeDate &gt; '10/30/16' and  DE.dtEventTimeDate &lt; '11/06/16')
          then dateadd(HH,-1,DE.dtEventTimeDate) 
          else DE.dtEventTimeDate
     End) EventTimeDate
FROM
    tblEmployee E
    join tblCardUser CU on CU.ixEmployee = E.ixEmployee -- OR CU.sExtraInfo = E.ixEmployee  CAN NOT ADD THIS LINE OR IT WILL SHOW SCANS FOR PREVIOUS EMPLOYEES WHO USED THAT SAME CARD!!!
    join tblCard C on C.ixCardUser = CU.ixCardUser
    join vwDoorEvent DE on DE.ixCardScanNum = C.ixCardScanNum
WHERE
        E.sPayrollId in (@Employee)
and DE.dtEventTimeDate &gt;= @StartDate
and DE.dtEventTimeDate &lt; (@EndDate+1)
and E.ixDepartment in (@Dept)
ORDER BY     E.sLastname,    (Case when (DE.dtEventTimeDate &gt; '10/30/16' and  DE.dtEventTimeDate &lt; '11/06/16')
          then dateadd(HH,-1,DE.dtEventTimeDate) 
          else DE.dtEventTimeDate
     End) </CommandText>
        <Timeout>9</Timeout>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="sFirstname">
          <DataField>sFirstname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sLastname">
