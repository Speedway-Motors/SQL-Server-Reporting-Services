    <DataSet Name="Department">
      <Query>
        <DataSourceName>SMISQLLIVE</DataSourceName>
        <CommandText>select distinct ixDepartment 
from tblEmployee E
    join tblCardUser CU on CU.ixEmployee = E.ixEmployee -- (only depts for employees that have a card)
where (ixDepartment between 20 and 29) -- verified returns dept (24) works under Call Center
  OR (ixDepartment in (4,45, 50))
order by ixDepartment</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ixDepartment">
          <DataField>ixDepartment</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="EmployeeID">
      <Query>
        <DataSourceName>SMISQLLIVE</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select distinct E.sPayrollId
from tblEmployee  E
    left join tblCardUser CU on CU.ixEmployee = E.ixEmployee
   left join tblCard C on C.ixCardUser = CU.ixCardUser
where E.ixDepartment in (@Dept)
order by E.sPayrollId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="sPayrollId">
          <DataField>sPayrollId</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
