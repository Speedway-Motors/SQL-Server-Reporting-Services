  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SMISQLLIVE</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SKU">
            <Value>=Parameters!SKU.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- sub-report QAV excluding Location 47
/*
DECLARE @SKU varchar(30)
SELECT  @SKU = '91089802'
*/
SELECT ixSKU, ixLocation 'Location', iQAV 'QAV'
FROM tblSKULocation
WHERE ixLocation &lt;&gt; 47 -- exclude AFCO
    and ixSKU = @SKU --('47552024','91089802','47522570','54785551','47552023','47553036')
ORDER BY ixSKU, ixLocation</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ixSKU">
          <DataField>ixSKU</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Location">
          <DataField>Location</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="QAV">
          <DataField>QAV</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
