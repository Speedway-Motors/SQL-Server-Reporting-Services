    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="FIFODataset">
      <Query>
        <DataSourceName>AFCOSQLLIVE</DataSourceName>
        <CommandText>  select
	FD.ixSKU as 'SKU',
	FD.iFIFOQuantity as 'Layer Qty',
	FD.mFIFOCost as 'Layer Unit Cost',
	--(FD.iFIFOQuantity*FD.mFIFOCost) as 'Extended Layer Cost',
    (cast (FD.iFIFOQuantity as money)) * cast(FD.mFIFOCost as money) as 'Extended Layer Cost',
	FD.sFIFOSourceType as 'Layer Source',
	case when FD.ixLocation=99 then 'Boonville' else 'Lincoln' end as 'Layer Location',
	convert(varchar, D.dtDate, 101) as 'Layer Date'
from
	tblFIFODetail FD
	left join tblDate D on FD.ixFIFODate = D.ixDate
order by
	FD.ixSKU</CommandText>
      </Query>
      <Fields>
        <Field Name="SKU">
          <DataField>SKU</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Layer_Qty">
          <DataField>Layer Qty</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Layer_Unit_Cost">
          <DataField>Layer Unit Cost</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Extended_Layer_Cost">
          <DataField>Extended Layer Cost</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Layer_Source">
          <DataField>Layer Source</DataField>
