<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="AFCOSQLLIVE">
      <DataSourceReference>AFCOSQLLIVE</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>95542fe7-556a-4bb7-a74d-4c5c5f4c6b90</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AFCOSQLLIVE</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT S.ixSKU 'SKU', VS.ixVendor 'PrimaryVendor',
    SL.iQOS 'QtyOnHand', S.ixMerchant 'Merchant',
    S.ixPGC 'PGC', --S.dtDateLastSOPUpdate -- 488
    FORMAT(S.dtDiscontinuedDate,'MM/dd/yy') 'Discontinued', 
    FORMAT(LS.LastSold,'MM/dd/yy') 'LastSold'
FROM tblSKU S
    left join tblVendorSKU VS on VS.ixSKU = S.ixSKU and VS.iOrdinality = 1
    left join tblSKULocation SL on SL.ixSKU = VS.ixSKU COLLATE SQL_Latin1_General_CP1_CS_AS and ixLocation = 99
    LEFT JOIN (-- LAST SOLD
        SELECT OL.ixSKU
                ,MAX(dtOrderDate) 'LastSold'
        FROM tblOrderLine OL 
                left join tblDate D on D.dtDate = OL.dtOrderDate 
        WHERE  OL.flgLineStatus IN ('Shipped','Dropshipped')
        GROUP BY OL.ixSKU
        ) LS on LS.ixSKU = S.ixSKU
WHERE S.flgDeletedFromSOP = 0
    and S.dtDiscontinuedDate between @StartDate and @EndDate -- &lt;= getdate()
    --and VS.ixVendor NOT IN ('9999','0999')
ORDER BY S.dtDiscontinuedDate, S.ixPGC, S.ixSKU</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="SKU">
          <DataField>SKU</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PrimaryVendor">
          <DataField>PrimaryVendor</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="QtyOnHand">
          <DataField>QtyOnHand</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Merchant">
          <DataField>Merchant</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PGC">
          <DataField>PGC</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Discontinued">
          <DataField>Discontinued</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastSold">
          <DataField>LastSold</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
